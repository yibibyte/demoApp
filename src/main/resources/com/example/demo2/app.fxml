<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jfxtras.scene.control.*?>

<VBox prefHeight="652.0" prefWidth="1044.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.AppController">
  <children>
    <MenuBar prefWidth="650.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Open…" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Delete" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About JavaDoc" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <LocalDateTimePicker fx:id="selectDateTime" layoutX="713.0" layoutY="23.0" prefHeight="282.0" prefWidth="307.0" />
            <Button fx:id="startScript" layoutX="448.0" layoutY="179.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="228.0" text="Выполнить сценарий резервное копирование" textAlignment="CENTER" wrapText="true" />
            <Button fx:id="chooseFileCopy" layoutX="52.0" layoutY="472.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0" text="Выбрать резервную копию" />
            <Button fx:id="chooseDirectory" layoutX="90.0" layoutY="149.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="165.0" text="Выбрать директорию" />
            <Button fx:id="deleteFileCopy" layoutX="90.0" layoutY="516.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="123.0" text="Удалить копию" />
            <LocalDateTimePicker fx:id="defaultDataTime" layoutX="713.0" layoutY="311.0" prefHeight="282.0" prefWidth="307.0" />
            <CheckBox fx:id="defaultPathFolder" layoutX="52.0" layoutY="107.0" mnemonicParsing="false" text="Сделатть по умолчанию для всех копий" />
            <TextField fx:id="pathFolder" layoutX="52.0" layoutY="63.0" prefHeight="30.0" prefWidth="524.0" />
            <TextField fx:id="nameBackupCopy" layoutX="81.0" layoutY="290.0" prefHeight="30.0" prefWidth="524.0" />
            <Label fx:id="labelPath" layoutX="52.0" layoutY="32.0" prefHeight="20.0" prefWidth="251.0" text="Путь к директории резервных копий:" />
      </children>
    </AnchorPane>
  </children>
</VBox>
